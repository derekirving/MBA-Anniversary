{
  "name": "src",
  "version": "0.0.1",
  "description": "src is a static site using Unify",
  "main": "./site/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:webpack": "cross-env UNIFY_ENV=development webpack --mode=development",
    "build:update-menus": "staticsites-update-menus",
    "dev:webpack": "cross-env UNIFY_ENV=development webpack --mode=development --watch",
    "dev:11ty": "cross-env UNIFY_ENV=development eleventy --serve",
    "dev:favicons": "cross-env UNIFY_ENV=development staticsites-favicons",
    "dist:clean": "node -e \"const fs = require('fs'); if (fs.existsSync('../wwwroot')) { fs.rmdirSync('../wwwroot', { recursive: true }); } process.exit(0);\"",
    "dist:webpack": "cross-env UNIFY_ENV=production webpack --mode production",
    "dist:favicons": "cross-env UNIFY_ENV=production staticsites-favicons",
    "dist:11ty": "cross-env UNIFY_ENV=production eleventy",
    "dist:index": "staticsites-index",
    "dist:archive": "staticsites-archive",
    "index": "staticsites-index",
    "createpot": "web-pot",
    "dotnet": "staticsites-dotnet",
    "dotdev": "cd .. && dotnet watch",
    "devnet": "run-p dev:** dotdev",
    "build": "run-p dev:favicons build:** index",
    "dev": "run-p dev:**",
    "dist": "npm-run-all dist:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "unify": {
    "i18n": {
      "paths": [
        "./site/**/*.@(ts|js|md)"
      ],
      "outputPath": "./app/localisation"
    }
  },
  "devDependencies": {
    "@unify/web": "^8.1.8",
    "@unify/web.staticsites": "^8.1.9",
    "@unify/web.theme.strathweb2022": "^8.0.14",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^5.1.4"
  }
}