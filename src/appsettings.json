{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Unify": {
    "i18n": {
      "DefaultCulture": "en-GB"
    },
    "StaticSite": {
      "SiteName": "MBA-Anniversary",
      "SiteShortName": "MBA Anniversary",
      "SiteDescription": "MBA Anniversary is is a Unify.Static website",
      "BackEndUrl": "https://content.sbs.strath.ac.uk",
      "PathPrefix": "/mba-anniversary",
      "CdnUrl": "https://content.sbs.strath.ac.uk",
      "InputDirectory": "site",
      "OutputDirectory": "wwwroot",
      "AssetsDirectory": "assets",
      "Pwa": false,
      "Background": "#fff",
      "Theme_Color": "#002b5c",
      "CheckLinks": {
        "enabled": true,
        "ignore": [
          "http://localhost",
          "https://localhost"
        ]
      }
    },
    "theme": {
      "versions": {
        "strathweb2022": "8.0.14"
      }
    }
  }
}